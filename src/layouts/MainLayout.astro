---
import HTML from "~/components/HTML.astro";
import Header from "~/components/Header.astro";

type Props = {
  title?: string;
  description?: string;
};

const { title, description } = Astro.props;
---

<HTML {title} {description}>
  <Fragment slot="body">
    <Header />
    <slot />
  </Fragment>
</HTML>
